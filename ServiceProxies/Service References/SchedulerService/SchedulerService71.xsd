<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.datacontract.org/2004/07/Scheduler.Estimated" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/Scheduler.Estimated" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="http://192.168.4.1/SchedulerService/SchedulerService.svc?xsd=xsd2" namespace="http://schemas.datacontract.org/2004/07/Scheduler" />
  <xs:import schemaLocation="http://192.168.4.1/SchedulerService/SchedulerService.svc?xsd=xsd5" namespace="http://schemas.datacontract.org/2004/07/Common" />
  <xs:import schemaLocation="http://192.168.4.1/SchedulerService/SchedulerService.svc?xsd=xsd6" namespace="http://schemas.datacontract.org/2004/07/TimeMeter" />
  <xs:complexType name="ArrayOfTasksDepenendency">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="TasksDepenendency" nillable="true" type="tns:TasksDepenendency" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfTasksDepenendency" nillable="true" type="tns:ArrayOfTasksDepenendency" />
