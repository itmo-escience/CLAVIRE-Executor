      <xs:extension xmlns:q2="http://schemas.datacontract.org/2004/07/Scheduler" base="q2:TaskScheduler.BaseTask">
        <xs:sequence>
          <xs:element minOccurs="0" name="Estimation" nillable="true" type="tns:ActiveEstimation" />
